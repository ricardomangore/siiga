.datagridBlack table { 
	border-collapse: collapse; 
	text-align: left; 
	width: 100%; 
} 
.datagridBlack {
	font: normal 12px/150% Arial, Helvetica, sans-serif; 
	background: #fff; 
	overflow: hidden; 
	-webkit-border-radius: 10px; 
	-moz-border-radius: 10px; 
	border-radius: 10px; 
	width: 40%;
	margin-left: 30%;	
	margin-top: 3%;
}
.datagridBlack table td, .datagridBlack table th { 
	padding: 5px 5px; 
}
.datagridBlack table thead th {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #000000), color-stop(1, #545454) );
	background:-moz-linear-gradient( center top, #000000 5%, #545454 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#000000', endColorstr='#545454');
	background-color:#000000; 
	color:#FFFFFF; 
	font-size: 16px; 
	font-weight: bold; 
	border-left: 0px solid #000000; 
} 
.datagridBlack table thead th:first-child { 
	border: none; 
}
.datagridBlack table tbody td { 
	color: #454545; 
	border-left: 1px solid #DBDBDB;
	font-size: 14px;font-weight: bold; 
}
.datagridBlack table tbody td:first-child { 
	border-left: none; 
}
.datagridBlack table tbody tr:last-child td { 
	border-bottom: none; 
}
.datagridBlack table tfoot td div { 
	border-top: 1px solid #000000;
	background: #A4A4A4;
} 
.datagridBlack table tfoot td { 
	padding: 0; 
	font-size: 13px 
} 
.datagridBlack table tfoot td div{ 
	padding: 7px; 
}
.datagridBlack table tfoot td ul { 
	margin: 0; 
	padding:0; 
	list-style: none; 
	text-align: right; 
}
.datagridBlack table tfoot  li { 
	display: inline; 
}

.datagridBlack table tbody label { 
	margin-left: 15%;
}

.datagridBlack table tfoot li a { 
	text-decoration: none; 
	display: inline-block;  
	padding: 2px 8px; 
	margin: 1px;
	color: #F5F5F5;
	border: 1px solid #8C8C8C;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #545454), color-stop(1, #000000) );
	background:-moz-linear-gradient( center top, #545454 5%, #000000 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#545454', endColorstr='#000000');
	background-color:#545454; 
}
.datagridBlack table tfoot ul.active, .datagridBlack table tfoot ul a:hover { 
	text-decoration: none;
	border-color: #7D7D7D; 
	color: #F5F5F5; 
	background: none; 
	background-color:#8C8C8C;
}
div.dhtmlx_window_active, div.dhx_modal_cover_dv { 
	position: fixed !important; 
}

