	function displayFormulario($ModuloId)
	{
		//Modo 1 -> Formulario
		//Modo 2 -> Edicion
		//Modo 3 -> Lista
		echo'<input type="hidden" id="Modo" name="Modo" value="1">

				<br>
				';
		switch ($ModuloId) {
			case '10':
						$this->displayPersonal();
				break;	
			case '22':
						$this->displayVentanilla();		
				break;			
			case '23':
						$this->displayRecepcion();
				break;
			case '24':
						$this->displayPrecaptura();		
				break;
			case '26':
						$this->displayVentana();
				break;
			default:
				echo '';
				break;
		}
	}//displayFormulario

