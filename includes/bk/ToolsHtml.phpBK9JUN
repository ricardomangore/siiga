	function DisplayControles($ModuloId)
	{
		$Permisos=$this->getPermisosModulo($ModuloId);
		echo'</br>
			 <div align="center">';
		if($ModuloId==0)
			echo'
				<input type="button" class="guardar" id="guardar" name="guardar">
				<input type="reset" value="" class="cancelar" id="cancelar" name="cancelar">
				';
		else
			{
					
					echo'					
					<input type="button" class="lista" id="lista" name="lista">
					<input type="button" class="formulario" id="formulario" name="formulario">
					';
					if(in_array("2", $Permisos)) 
						echo'
							<input type="button" class="nuevo" id="nuevo" name="nuevo">
							<input type="button" class="guardar" id="guardar" name="guardar">
							';				
