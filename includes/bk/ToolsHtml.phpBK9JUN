						echo'
							<input type="button" class="editar" id="editar" name="editar">
							<input type="button" class="guardar" id="actualizar" name="actualizar">
							';
					if(in_array("3", $Permisos))
						echo'					
							<input type="button" class="borrar" id="borrar" name="borrar">
							';
					if(in_array("4", $Permisos))
						echo'					
							<input type="button" class="activar" id="activar" name="activar">
							';
					
					echo'
					<input type="reset" value="" class="cancelar" id="cancelar" name="cancelar">
					';
			}
		echo'</div>';	
	}

