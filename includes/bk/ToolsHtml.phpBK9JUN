		while($A0=mysql_fetch_row($R0)) 
		{		
		if($i==0)
				{
					echo'<thead class="fixedHeader"><tr>';
						for($j=0; $j<$Orden['columnas']; $j++)
							{
							if ($j==0)								
								{
										echo '<td bgcolor="#BFD5EA" align="left" valign="midle" width="50" >
