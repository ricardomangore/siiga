					FROM HFolios AS T1
					LEFT JOIN PuntosVenta AS T2 ON T2.PuntoVentaId=T1.PuntoVentaId
					LEFT JOIN Plazas AS T3 ON T3.PlazaId=T2.PlazaId
					LEFT JOIN SubRegiones AS T4 ON T4.SubRegionId=T3.SubRegionId
