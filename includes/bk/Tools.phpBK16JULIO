					LEFT JOIN (SELECT Folio, COUNT(RegistroId) AS Lineas FROM LFolios GROUP BY Folio) AS T7 ON T7.Folio=T1.Folio
					WHERE T1.PuntoventaId IN ($MisPuntos) AND MovimientoId>0 
