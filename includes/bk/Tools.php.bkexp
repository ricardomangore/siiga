		LEFT JOIN SubCategorias AS T9 ON T9.SubCategoriaId=T7.SubCategoriaId
		LEFT JOIN Empleados AS T10 ON T10.EmpleadoId=T7.EmpleadoId
		LEFT JOIN Clientes AS T11 ON T11.ClienteId=T1.ClienteId
