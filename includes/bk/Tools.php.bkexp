		            LEFT JOIN Estatus AS T7 ON T7.EstatusId=T1.EstatusId
		            LEFT JOIN Equipos AS T9 ON T9.EquipoId=T1.EquipoId
		            LEFT JOIN Marcas AS T10 ON T10.MarcaId=T9.MarcaId
