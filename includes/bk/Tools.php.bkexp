	function CambiarFormatoFecha($Fecha){ 
	    list($Dia,$Mes,$Anio)=explode("/",$Fecha); 
   		return $Anio."-".$Mes."-".$Dia; 
	}//CambiarFormatoFecha

	function StartTransaccion()
	{
		$Q0="SET AUTOCOMMIT=0";
